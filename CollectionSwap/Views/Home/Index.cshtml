    @*<section class="disclaimer">
        🛡️ Safety First! 🚀<br />
        At Swapper, we're all about fun and safe trading. Our platform is exclusively for collecting and swapping items. Be kind, keep it digital, and enjoy your trading adventures with peace of mind!
    </section>*@
    <section class="add-sponsor">
        <div class="sponsors">
            @if (sponsors.Count > 0)
            {
                <div class="header">
                    <span>Proudly partnered with:</span>

                    @if (User.IsInRole("Admin"))
                    {
                        <button class="edit-sponsors-button" onclick="EditHomeSponsors(this)">Edit</button>
                    }
                </div>
                <div class="body">
                    @foreach (var sponsor in sponsors)
                    {
                        var url = sponsor.Url == null ? "#" : sponsor.Url.StartsWith("http://") || sponsor.Url.StartsWith("https://") ? sponsor.Url : $"http://{sponsor.Url}";
                        <div>
                            <a href="@url"><img data-id="@sponsor.Id" class="sponsor-image" src="/Sponsors/0/@sponsor.Image" /></a>
                            @if (User.IsInRole("Admin"))
                            {
                                using (Html.BeginForm("DeleteHomePageSponsor", "Manage", new { id = sponsor.Id }, FormMethod.Post))
                                {
                                    @Html.AntiForgeryToken()
                                    <button class="button-icon button-icon-trash" type="submit" onclick="return confirm('Are you sure you want to remove this sponsor?')"><i class="fa-solid fa-trash"></i></button>
                                }
                            }
                        </div>
                    }
                </div>
            }
            else if (User.IsInRole("Admin"))
            {
                <div class="header">
                    <span>Proudly partnered with:</span>

                    <button class="edit-sponsors-button" onclick="EditHomeSponsors(this)">Edit</button>
                </div>
                <div class="body">
                    Currently no partners
                </div>
            }
        </div>

        @if (User.IsInRole("Admin"))
        {
            using (Html.BeginForm("EditHomePageSponsor", "Manage", null, FormMethod.Post, new { enctype = "multipart/form-data", id = "edit-sponsor-form" }))
            {
                @Html.AntiForgeryToken()
                <span class="header">Edit Sponsor</span>
                <div>
                    <div class="form-group">
                        @Html.Label("Image")
                        <label class="admin placeholder" for="edit-sponsor-image-upload">
                            <img class="sponsor-image placeholder" src="~/Content/images/scrollbar.png" alt="Sponsor Logo" />
                        </label>
                        <input class="d-none" type="file" id="edit-sponsor-image-upload" name="fileInput" accept=".png, .jpg">
                        @Html.ValidationMessageFor(model => model.FileInput)
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.Url)
                        @Html.TextBoxFor(model => model.Url)
                        @Html.ValidationMessageFor(model => model.Url)
                    </div>
                </div>
                @Html.Hidden("Id", -1)
                <div class="row gap-1">
                    <input class="submit-button" type="submit" value="Edit Sponsor" />
                    <input class="submit-button clear" type="reset" value="Clear" />
                </div>
            }

            using (Html.BeginForm("AddHomePageSponsor", "Manage", null, FormMethod.Post, new { enctype = "multipart/form-data", id = "add-sponsor-form" }))
            {
                @Html.AntiForgeryToken()
                <span class="header">Add Sponsor</span>
                <div>
                    <div class="form-group">
                        @Html.Label("Image")
                        <label class="admin placeholder" for="add-sponsor-image-upload">
                            <img class="sponsor-image placeholder" src="~/Content/images/scrollbar.png" alt="Sponsor Logo" />
                        </label>
                        <input class="d-none" type="file" id="add-sponsor-image-upload" name="fileInput" accept=".png, .jpg">
                        @Html.ValidationMessageFor(model => model.FileInput)
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.Url)
                        @Html.TextBoxFor(model => model.Url)
                        @Html.ValidationMessageFor(model => model.Url)
                    </div>
                </div>
                @Html.Hidden("Id", -1)
                <div class="row gap-1">
                    <input class="submit-button" type="submit" value="Add Sponsor" />
                    <input class="submit-button clear" type="reset" value="Clear" />
                </div>
            }
        }
    </section>
</div>

